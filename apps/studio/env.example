# Voice-First API Keys for AuraOS Studio
# Copy this file to .env and add your actual API keys

# Deepgram API Key for cloud STT
REACT_APP_DEEPGRAM_API_KEY=your_deepgram_api_key_here

# ElevenLabs API Key for TTS
REACT_APP_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# OpenAI API Key (alternative for TTS)
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Optional: Custom voice IDs
REACT_APP_ELEVENLABS_VOICE_ID=your_voice_id_here
REACT_APP_ELEVENLABS_ARABIC_VOICE_ID=your_arabic_voice_id_here
