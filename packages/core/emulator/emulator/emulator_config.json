{
  "emulator": {
    "name": "Py65 6502 Emulator",
    "version": "1.2.0",
    "type": "py65",
    "memory_size": 65536,
    "cpu_speed": 1000000,
    "debug_mode": true,
    "memory_regions": {
      "zero_page": {
        "start": "0x0000",
        "end": "0x00FF",
        "description": "CPU registers and system variables"
      },
      "stack": {
        "start": "0x0100",
        "end": "0x01FF",
        "description": "CPU stack area"
      },
      "basic_program": {
        "start": "0x8000",
        "end": "0xFFFF",
        "description": "BASIC program area"
      },
      "display": {
        "start": "0x2000",
        "end": "0x20FF",
        "description": "Display output buffer"
      }
    },
    "cpu_registers": {
      "pc": "Program Counter",
      "a": "Accumulator",
      "x": "X Register",
      "y": "Y Register",
      "sp": "Stack Pointer",
      "status": "Status Register"
    },
    "status_flags": {
      "carry": "Carry Flag (C)",
      "zero": "Zero Flag (Z)",
      "interrupt": "Interrupt Disable Flag (I)",
      "decimal": "Decimal Mode Flag (D)",
      "break": "Break Command Flag (B)",
      "overflow": "Overflow Flag (V)",
      "negative": "Negative Flag (N)"
    },
    "basic_commands": {
      "supported": [
        "PRINT",
        "LET",
        "FOR",
        "NEXT",
        "IF",
        "THEN",
        "GOTO",
        "GOSUB",
        "RETURN",
        "END",
        "REM",
        "DATA",
        "READ"
      ],
      "execution": {
        "timeout": 30,
        "max_steps": 10000,
        "debug_steps": 100
      }
    },
    "integration": {
      "python_interface": true,
      "bridge_layer": true,
      "ai_layer": true,
      "ui_layer": true,
      "settings_integration": true
    },
    "performance": {
      "optimization": true,
      "caching": true,
      "async_execution": false,
      "memory_monitoring": true
    },
    "logging": {
      "level": "INFO",
      "file": "logs/emulator.log",
      "console": true,
      "debug_trace": false
    }
  }
}
