{
  "name": "@auraos/core",
  "version": "1.0.0",
  "description": "AuraOS Core - Kernel and system components",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.mjs",
      "require": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auraos/ai": "workspace:*",
    "@auraos/firebase": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "keywords": ["auraos", "core"],
  "author": "Mohamed Abdelaziz",
  "license": "MIT"
}
