{
  "name": "@auraos/firebase",
  "version": "1.0.0",
  "type": "module",
  "description": "AuraOS Firebase - Authentication, Firestore, and AI integration",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
