{
  "name": "@auraos/workers",
  "version": "1.0.0",
  "description": "AuraOS Workers - Background job processing with BullMQ",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
